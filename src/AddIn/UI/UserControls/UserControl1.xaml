<UserControl x:Class="Chem4Word.UI.UserControls.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Chem4Word.UI.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_Loaded">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="10,10,0,128" TextWrapping="Wrap" Text="TextBox" Width="263" />
        <Button x:Name="button" Content="Button" Height="54" Margin="0,0,27,23" VerticalAlignment="Bottom" RenderTransformOrigin="0.638,0.771" HorizontalAlignment="Right" Width="122" Click="button_Click" />
    </Grid>
</UserControl>